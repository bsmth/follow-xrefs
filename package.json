{
  "name": "follow-xrefs",
  "displayName": "Follow MDN slugs",
  "publisher": "bsmth",
  "description": "Jump to documents in VS Code based on MDN slugs",
  "version": "1.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/bsmth/follow-xrefs"
  },
  "bugs": {
    "url": "https://github.com/bsmth/follow-xrefs/issues"
  },
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "MDN Web Docs",
    "Markdown",
    "Documentation"
  ],
  "license": "SEE LICENSE IN LICENSE.txt",
  "vsce": {
    "yarn": false
  },
  "pricing": "Free",
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "follow-xrefs.scanFiles",
        "title": "Index MDN slugs"
      }
    ]
  },
  "scripts": {
    "lint": "eslint . && prettier --check ./",
    "pretest": "npm run lint",
    "test": "node --test",
    "deploy": "vsce publish"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.96.0",
    "@vscode/vsce": "^3.2.1",
    "eslint": "^9.16.0",
    "prettier": "^3.4.2"
  }
}
